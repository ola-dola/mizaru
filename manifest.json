{
  "manifest_version": 3,
  "name": "Mizaru - See No Evil",
  "version": "0.0.1",
  "author": "Ola Oredola",
  "description": "Remove video suggestions from YouTube's home page and links to 'Shorts'",
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "48": "assets/48.png",
    "96": "assets/96.png",
    "128": "assets/128.png"
  }
}